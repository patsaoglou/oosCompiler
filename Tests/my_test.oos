class Circle inherits Shape:
    int radius;
    int color;
    ;

    def __init__(self, int radius): Circle
        self.radius = radius;
        return self;
        ;

    def __init__(self, int x, int y, int color, int radius): Circle
        self.x = x;
        self.y = y;
        self.color = color;
        self.radius = radius;
        return self;
        ;

    def get_radius(self): int
        return self.radius;
        ;

    def area(self): int
        int int_pi;
        ;
        int_pi = 3;
        return int_pi * self.radius * self.radius;
        ;

