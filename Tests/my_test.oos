class Shape:
    int x, y;
    int color;
    ;

    def __init__(self, int x, int y): Shape
        self.x = x;
        self.y = y;
        return self;
        ;

    def __init__(self, int x, int y, int color): Shape
        self.x = x;
        self.y = y;
        self.color = color;
        return self;
        ;

    def move(self, int dx, int dy): -
        self.x = self.x + dx;
        self.y = self.y + dy;
        ;

    def move(self, int dx): -
        self.x = self.x + dx;
        ;

    def set_x(self, int x): -
        self.x = x;
        ;

    def get_x(self): int
        return self.x;
        ;

